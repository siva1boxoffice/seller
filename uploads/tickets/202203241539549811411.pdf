<script>
    $(document).ready(function () {
        if ($.cookie('loginEmail') && $.cookie('loginPassword')) {
            $("#loginEmail").val($.cookie('loginEmail'));
            $("#loginPassword").val($.cookie('loginPassword'));
            $('input#rememberpassword').prop('checked', true);
        } else {
            emptyValue();
        }
        $('#submitLogin').click(function () {

            if ($('input#rememberpassword').is(':checked')) {
                $.cookie('loginEmail', $("#loginEmail").val());
                $.cookie('loginPassword', $("#loginPassword").val());
            } else {
                emptyValue();
            }
        });
    });
    function emptyValue() {
        $.cookie('loginEmail', '');
        $.cookie('loginPassword', '');
    }



    $("#loginForm").validate({
        rules: {
            email: {
                required: true,
                email: true

