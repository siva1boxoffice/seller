        },
        messages: {
            email: {required: "<span class='error-message'>Enter your email.</span>",
                email: "<span class='error-message' >Enter a correct e-mail address.</span>",
            },
            password: {required: "<span class='error-message'>Enter Your Password.</span>"},
        },
    });
</script>

